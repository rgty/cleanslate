[QUERY]
social_sql=select doc_id, wdfsi.feed_id, (case when social_interaction_type_id = 4 then 1 when social_interaction_type_id = 1 then 2 when social_interaction_type_id = 2 then  3 end) rate from wc_doc_feed_social_interaction wdfsi join wc_feed wf on wdfsi.feed_id = wf.feed_id and feed_type_id = 1 and social_interaction_type_id != 3 group by doc_id, wdfsi.feed_id
speciality_sql=select doc_id, ws.speciality_name speciality from wc_doc_speciality wds join wc_speciality ws on wds.speciality_id = ws.speciality_id